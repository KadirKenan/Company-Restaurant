<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>@ViewData["Title"] - FOOD</title>
    <title>Foodtech Dashboard</title>
    <!-- Iconic Fonts -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="~/hazırcss/vendors/iconic-fonts/font-awesome/css/all.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/hazırcss/vendor/iconic-fonts/flat-icons/flaticon.css">
    <link rel="stylesheet" href="~/hazırcss/vendors/iconic-fonts/cryptocoins/cryptocoins.css">
    <link rel="stylesheet" href="~/hazırcss/vendors/iconic-fonts/cryptocoins/cryptocoins-colors.css">
    <!-- Bootstrap core CSS -->
    <link href="~/hazırcss/assets/css/bootstrap.min.css" rel="stylesheet">
    <!-- jQuery UI -->
    <link href="~/hazırcss/assets/css/jquery-ui.min.css" rel="stylesheet">
    <!-- Page Specific CSS (Slick Slider.css) -->
    <link href="~/hazırcss/assets/css/slick.css" rel="stylesheet">
    <link href="~/hazırcss/assets/css/datatables.min.css" rel="stylesheet">
    <!-- Foodtech styles -->
    <link href="~/hazırcss/assets/css/style.css" rel="stylesheet">
    <!-- Favicon -->
    <link rel="icon" type="image/png" sizes="32x32" href="~/hazırcss/favicon.ico">
</head>

<body class="ms-body ms-aside-left-open ms-primary-theme ms-has-quickbar">
    <header>
        <nav class="navbar ms-navbar">

            <div class="ms-aside-toggler ms-toggler pl-0" data-target="#ms-side-nav" data-toggle="slideLeft">
                <span class="ms-toggler-bar bg-primary"></span>
                <span class="ms-toggler-bar bg-primary"></span>
                <span class="ms-toggler-bar bg-primary"></span>
            </div>

            <div class="logo-sn logo-sm ms-d-block-sm">
                <a class="pl-0 ml-0 text-center navbar-brand mr-0" href=""><img src="~hazırcss/assets/img/foodtech/foodtech-logo-84x41.png" alt="logo"> </a>
            </div>

            <ul class="ms-nav-list ms-inline mb-0" id="ms-nav-options">
                <li>
                    <partial name="_AdminNavbarPartial" />
                </li>
                <li class="ms-nav-item ms-search-form pb-0 py-0">
                    <form class="ms-form" method="post">
                        <div class="ms-form-group my-0 mb-0 has-icon fs-14">
                            <input type="search" class="ms-form-input" name="search" placeholder="Search here..." value="">
                            <i class="flaticon-search text-disabled"></i>
                        </div>
                    </form>
                </li>

                <partial name="_AdminNavbarPartial" />

            </ul>



        </nav>
    </header>
    <div class="container">
        <!-- Overlays -->
        <div class="ms-aside-overlay ms-overlay-left ms-toggler" data-target="#ms-side-nav" data-toggle="slideLeft"></div>
        <div class="ms-aside-overlay ms-overlay-right ms-toggler" data-target="#ms-recent-activity" data-toggle="slideRight"></div>
        <!-- Sidebar Navigation Left -->
        <aside id="ms-side-nav" class="side-nav fixed ms-aside-scrollable ms-aside-left">
            <!-- Logo -->
            <div class="logo-sn ms-d-block-lg">
                <a class="pl-0 ml-0 text-center" href="~/WarehouseAttendant/Profile/Profile">
                    <img src="~/hazırcss/assets/img/foodtech/foodtech-logo-216x62.png" alt="logo">
                </a>
            </div>
            <!-- Navigation -->
            <ul class="accordion ms-main-aside fs-14" id="side-nav-accordion">
                <!-- Dashboard -->
                <li class="menu-item">
                    <a href="#" class="has-chevron" data-toggle="collapse" data-target="#dashboard" aria-expanded="false" aria-controls="dashboard">
                        <span><i class="material-icons fs-16">dashboard</i>Dashboard </span>
                    </a>
                    <ul id="dashboard" class="collapse" aria-labelledby="dashboard" data-parent="#side-nav-accordion">
                        <li>
                            <a href="~/WarehouseAttendant/Profile/Profile">Profil</a>
                        </li>
                    </ul>
                </li>
                <!-- /Dashboard -->
                <!-- orders -->
                <li class="menu-item">
                    <a href="~/WarehouseAttendant/Order/Index">
                        <span><i class="fas fa-clipboard-list fs-16"></i>Siparişler</span>
                    </a>
                </li>
                <!-- orders end -->
                <!-- StorageProduct -->
                <li class="menu-item">
                    <a href="~/WarehouseAttendant/StorageProduct/Index">
                        <span><i class="fas fa-clipboard-list fs-16"></i>Depo Ürün Listesi</span>
                    </a>
                </li>
                <!-- StorageProduct end -->
                <li class="menu-item">
                    <a href="~/WarehouseAttendant/StorageCategory/Index">
                        <span><i class="fas fa-clipboard-list fs-16"></i>Depo Ürün Kategori Listesi</span>
                    </a>
                </li>
                <!-- Supplier -->
                <li class="menu-item">
                    <a href="~/WarehouseAttendant/Supplier/Index">
                        <span><i class="fa fa-tasks fs-16"></i>Tedarikçi Listesi</span>
                    </a>
                </li>
                <!-- Supplier end -->





            </ul>
        </aside>
    </div>
    <!-- Sidebar Right -->
    @RenderBody()
    <!-- Global Required Scripts Start -->
    <script src="~/hazırcss/assets/js/jquery-3.3.1.min.js"></script>
    <script src="~/hazırcss/assets/js/popper.min.js"></script>
    <script src="~/hazırcss/assets/js/bootstrap.min.js"></script>
    <script src="~/hazırcss/assets/js/perfect-scrollbar.js">
    </script>
    <script src="~/hazırcss/assets/js/jquery-ui.min.js">
    </script>
    <!-- Global Required Scripts End -->
    <!-- Page Specific Scripts Start -->

    <script src="~/hazırcss/assets/js/Chart.bundle.min.js">
    </script>
    <script src="~/hazırcss/assets/js/widgets.js"></script>
    <script src="~/hazırcss/assets/js/clients.js"></script>
    <script src="~/hazırcss/assets/js/Chart.Financial.js"></script>
    <script src="~/hazırcss/assets/js/d3.v3.min.js">
    </script>
    <script src="~/hazırcss/assets/js/topojson.v1.min.js">
    </script>
    <script src="~/hazırcss/assets/js/datatables.min.js">
    </script>
    <script src="~/hazırcss/assets/js/data-tables.js">
    </script>
    <!-- Page Specific Scripts Finish -->
    <!-- Foodtech core JavaScript -->
    <script src="~/hazırcss/assets/js/framework.js"></script>
    <!-- Settings -->
    <script src="~/hazırcss/assets/js/settings.js"></script>
</body>

</html>
